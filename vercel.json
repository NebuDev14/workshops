{
  "public": true,
  "github": { "silent": true },
  "rewrites": [
    {
      "source": "/workshops/_next/:path*",
      "destination": "/_next/:path*"
    }
  ],
  "headers": [
    {
      "source": "/api/patterns/(.+)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=5000, immutable" }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ]
}
